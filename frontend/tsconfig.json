{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {

    "jsx": "react-jsx",
    "types": ["react", "react-native"],
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@assets/*": ["./assets/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",

    "../.expo/types/**/*.ts",
    "expo-env.d.ts",
    ".expo/types/**/*.ts"

  ]
}